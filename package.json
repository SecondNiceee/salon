{
  "name": "my-shop",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_ENV=production NODE_OPTIONS=--no-deprecation next start -H 0.0.0.0 -p 3000",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "seed:cities": "cross-env NODE_OPTIONS=--no-deprecation npx tsx scripts/seed-cities.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.0",
    "@payloadcms/db-postgres": "3.49.0",
    "@payloadcms/email-nodemailer": "^3.50.0",
    "@payloadcms/live-preview-react": "^3.54.0",
    "@payloadcms/next": "3.49.0",
    "@payloadcms/payload-cloud": "3.49.0",
    "@payloadcms/richtext-lexical": "3.49.0",
    "@payloadcms/ui": "3.49.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.15",
    "@types/leaflet": "^1.9.20",
    "axios": "^1.11.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "graphql": "^16.8.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.532.0",
    "next": "15.3.6",
    "next-themes": "^0.4.6",
    "nodemailer": "^7.0.5",
    "payload": "3.49.0",
    "react": "19.2.1",
    "react-hook-form": "^7.61.1",
    "react-leaflet": "^5.0.0",
    "sass": "^1.89.2",
    "scss": "^0.2.4",
    "sharp": "0.34.2",
    "sonner": "^2.0.7",
    "swr": "^2.3.7",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
    "zod": "^4.0.11",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@playwright/test": "1.50.0",
    "@shadcn/ui": "^0.0.4",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.5.4",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "19.2.1",
    "@types/react-dom": "19.2.1",
    "@types/uuid": "^11.0.0",
    "@vitejs/plugin-react": "4.5.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.0",
    "jsdom": "26.1.0",
    "lorem-ipsum": "^2.0.8",
    "playwright": "1.50.0",
    "playwright-core": "1.50.0",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "react-dom": "19.2.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.21.0",
    "tw-animate-css": "^1.3.6",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}